  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": [
        "CVE-2025-69873",
        "CVE-2026-26996"
      ]
    },
    "overrides": {
      "glob>minimatch": ">=10.2.1",
      "filelist>minimatch": ">=10.2.1",
      "@typescript-eslint/typescript-estree>minimatch": ">=10.2.1",
      "qs": ">=6.14.2",
      "workbox-build>ajv": ">=8.18.0",
      "@modelcontextprotocol/sdk>ajv": ">=8.18.0"
    }
