{
  "name": "hadrian-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test-storybook": "vitest run --project=storybook --project=storybook-dark",
    "test-storybook:light": "vitest run --project=storybook",
    "test-storybook:dark": "vitest run --project=storybook-dark",
    "test": "vitest run --project=unit",
    "test:watch": "vitest --project=unit",
    "generate-api": "openapi-ts -f openapi-ts.config.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
